{
  "name": "n8n-nodes-hanna",
  "version": "1.2.0",
  "description": "n8n community nodes for Hanna IRC Bot integration",
  "keywords": [
    "n8n-community-node-package",
    "irc",
    "bot",
    "chat",
    "hanna"
  ],
  "license": "GPLv3",
  "homepage": "https://github.com/h4ks-com/hanna",
  "author": {
    "name": "H4KS",
    "email": "contact@h4ks.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/h4ks-com/hanna.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts'",
    "lintfix": "eslint 'nodes/**/*.ts' 'credentials/**/*.ts' --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/HannaBotApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/HannaBot/HannaBot.node.js",
      "dist/nodes/HannaBotTrigger/HannaBotTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.40.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
